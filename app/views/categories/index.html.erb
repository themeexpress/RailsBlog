<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
    <h1>Category List</h1>
    <hr/>
    <%= will_paginate %>
    <% @categories.each do |category| %>
    <ul class="listing">
      <li class="list-title">
      <%= link_to  "#{category.name}", category_path(category) %>
      </li>
    </ul>
    <% end %>
    </div>
  </div>
</div>