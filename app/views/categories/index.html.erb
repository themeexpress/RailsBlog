<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1>Category List</h1>
      <hr/>
      <%= will_paginate %>
      <% @categories.each do |category| %>
      <div class="panel panel-default">
        <div class="panel-body">
        <%= link_to  "#{category.name}", category_path(category) %>
        </div>
      </div>
      <% end %>
      <%= will_paginate %>
    </div>
  </div>
</div>