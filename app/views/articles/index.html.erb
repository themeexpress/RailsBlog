<h1>Listing All posts</h1>
<%= link_to  "Create New Post", new_article_path %>
<table>
<tr>
  <th>Title</th>
  <th>Description</th>
  <th>Actions</th>
</tr>
<% @articles.each do |article| %>
<tr>
  <td><%= article.title %></td>
  <td><%= article.description %></td>
  <td><%= link_to "Edit", edit_article_path(article) %> | <%= link_to "Show", article_path(article) %>
    
  </td>
</tr>
<% end %>
</table>